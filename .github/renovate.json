{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":automergeAll",
    ":dependencyDashboard",
    ":rebaseStalePrs",
    ":semanticCommits",
    ":semanticPrefixFixDepsChoreOthers",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "packageRules": [
    {
      "matchFiles": [
        "MODULE.bazel", 
        "bazel_integration_test/deps.bzl"
      ],
      "enabled": false
    }
  ]
}
